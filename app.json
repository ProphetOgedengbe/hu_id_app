{
  "expo": {
    "name": "IDApp",
    "slug": "idapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.idapp",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture your ID photo.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library so you can choose an image for your ID."
      }
    },
    "android": {
      "package": "com.yourname.idapp",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e52ec4d4-fed9-4f56-aa56-da1a3c9bcae0"
      }
    },
    "owner": "lineardavid"
  }
}